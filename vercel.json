{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(css|img|js)/(.*)",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/api-docs.html",
      "dest": "/public/api-docs.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
} 